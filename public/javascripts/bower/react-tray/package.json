  },
  "devDependencies": {
    "browserify": "^10.1.3",
    "browserify-shim": "^3.8.6",
    "envify": "^3.4.0",
    "jsx-loader": "^0.13.1",
    "karma": "^0.12.31",
    "karma-browserify": "^4.1.2",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "mocha": "^2.2.4",
